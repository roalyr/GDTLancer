[application]

config/name="GDTLancer-rev"
config/description="GDTLancer revised"
run/main_scene="res://scenes/levels/game_world/main_game_scene.tscn"
boot_splash/image="res://Splash.png"
config/icon="res://Icon.png"
config/windows_native_icon="res://Icon.ico"

[autoload]

Constants="*res://src/autoload/Constants.gd"
EventBus="*res://src/autoload/EventBus.gd"
GlobalRefs="*res://src/autoload/GlobalRefs.gd"
TemplateDatabase="*res://src/autoload/TemplateDatabase.gd"
GameState="*res://src/autoload/GameState.gd"
CoreMechanicsAPI="*res://src/autoload/CoreMechanicsAPI.gd"
