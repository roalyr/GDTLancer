window/size/height=1080
window/stretch/mode="2d"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PoolStringArray( "res://addons/gdformat/plugin.cfg", "res://addons/gut/plugin.cfg" )

[gui]

